{
    "id": "star-wars-status-automation",
    "version": "#{VERSION}#",
    "url": "#{URL}#",
    "manifest": "#{MANIFEST}#",
    "title": "FFG Star Wars Status Automation",
    "description": "Various quality-of-life status automations for the FFG Star Wars system",
    "license": "LICENSE",
    "readme": "README.md",
    "compatibility": {
        "minimum": "12",
        "verified": "12",
        "maximum": "13"
    },
    "authors": [
        {
            "name": "Alix Deltour",
            "url": "https://github.com/Malven31",
            "discord": "malven31#0013"
        }
    ],
    "languages": [
        {
            "lang": "en",
            "name": "English",
            "path": "lang/en.json"
        },
        {
            "lang": "fr",
            "name": "French",
            "path": "lang/fr.json"
        }
    ],
    "esmodules": ["star-wars-status-automation.js"],
    "relationships": {
        "requires": [
            {
                "id": "statuscounter",
                "type": "module",
                "compatibility": {}
            }
        ],
        "systems": [
            {
                "id": "starwarsffg",
                "type": "system",
                "compatibility": {}
            }
        ]
    },
    "flags": {
        "hotReload": {
            "extensions": ["json"],
            "paths": ["lang"]
        }
    }
}
